{
  "tabWidth": 2,
  "semi": false,
  "printWidth": 80,
  "singleQuote": true,
  "endOfLine": "auto",
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "trailingComma": "none",
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrder": [
    "^.$|^./types$|^./styles|^./logic$",
    "^./|^../",
    "@expo|react-native",
    "^@src/styles",
    "^@src/screens",
    "^@src/components",
    "^@src/routes",
    "^@src/shared",
    "^@src/static",
    "^@src/pages",
    "^@src/hooks",
    "^@src/hocs",
    "^@src/store",
    "^@src/services",
    "^@src/tests",
    "^@src/types",
    "^@src/utils",
    "^@public",
    "^@common",
    "<THIRD_PARTY_MODULES>"
  ]
}
